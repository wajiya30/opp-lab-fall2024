//Task 1
package firstproject1;
class Student {
    String name;
    String city;
    int age;
}

public class FirstProject1 {
    public static void main(String[] args) {
        // Create and initialize two Student objects
        Student s1 = new Student();
        s1.name = "Wajeeha";
        s1.city = "Rwp";
        s1.age = 20;

        Student s2 = new Student();
        s2.name = "Fatima";
        s2.city = "ISL";
        s2.age = 20;

 System.out.println("Student 1 Details:");
        System.out.println("Name: " + s1.name);
        System.out.println("City: " + s1.city);
        System.out.println("Age: " + s1.age);

        // Print the details of the second student
        System.out.println("\nStudent 2 Details:");
        System.out.println("Name: " + s2.name);
        System.out.println("City: " + s2.city);
        System.out.println("Age: " + s2.age);
    }
    
}
//Task 2
package secondproject;
class Area {
    // Attributes
    double length;
    double width;
    double calculateArea() {
        return length * width;
    }
    void displayArea() {
        System.out.println("Area: " + calculateArea());
    }
}

public class SecondProject {

    public static void main(String[] args) {
       Area area = new Area();

        area.length = 5.0; 
        area.width = 3.0; 

        area.displayArea();  
    }
}
//Task 3
package thirdproject;
class Computer {
    // Attributes
    String name;
    String config;
    double cost;
    String os;
         void displayDetails() {
        System.out.println("The company name is " + name);
        System.out.println("The configuration is " + config);
        System.out.println("Its Cost is " + cost);
        System.out.println("Its operating System is " + os);
         }
}
 
public class ThirdProject {

    public static void main(String[] args) {
        Computer computer = new Computer();

        computer.name = "Apple";
        computer.config = "i5";
        computer.cost = 50000;
        computer.os = "IOS";

        // Display the details of the computer
        computer.displayDetails();
    }
    
}
//Task 4
public class MaximumNumber {
    public static void main(String[] args) {
       
        int num1 = 10;
        int num2 = 35;
        int num3 = 25;

        
        int max;
        if (num1 >= num2 && num1 >= num3) {
            max = num1;
        } else if (num2 >= num1 && num2 >= num3) {
            max = num2;
        } else {
            max = num3;
        }

       
        System.out.println("The maximum number is: " + max);
    }
}

